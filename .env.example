# Tastytrade API Credentials Template
# Copy this file to .env and fill in your actual credentials

# Your client secret from Tastytrade
TASTYTRADE_CLIENT_SECRET=your_client_secret_here

# Your refresh token from Tastytrade
TASTYTRADE_REFRESH_TOKEN=your_refresh_token_here

# Optional: API base URL (default production)
TASTYTRADE_API_URL=https://api.tastytrade.com
